<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.twih.front"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.twih.front.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.twih.front.ChatListActivity" />
        <activity android:name="com.example.twih.front.ChatRoomActivity" />
        <activity android:name="com.example.twih.front.FriendListActivity" />
        <activity android:name="com.example.twih.front.GetAddressActivity" />
        <activity android:name="com.example.twih.front.SignUpActivity" />
        <activity android:name="com.example.twih.front.SettingActivity" />
    </application>

</manifest>